file = {:Users/anders/Dropbox/Litteratur/Sant et al. - 2010 - Incidence of hematologic malignancies in Europe by morphologic subtype results of the HAEMACARE project.pdf:pdf},
issn = {1528-0020},
journal = {Blood},
keywords = {Europe,Europe: epidemiology,Female,Hematologic Neoplasms,Hematologic Neoplasms: classification,Hematologic Neoplasms: epidemiology,Humans,Incidence,Leukemia,Leukemia: classification,Leukemia: epidemiology,Lymphoma,Lymphoma: classification,Lymphoma: epidemiology,Male,Myelodysplastic-Myeloproliferative Diseases,Myelodysplastic-Myeloproliferative Diseases: class,Myelodysplastic-Myeloproliferative Diseases: epide,Registries},
