issn = {1367-4803},
journal = {Bioinformatics (Oxford, England)},
keywords = {Animals,Cell Physiological Phenomena,Computational Biology,Computational Biology: methods,Computer Simulation,Gene Expression Profiling,Gene Expression Profiling: methods,Gene Expression Regulation,Gene Expression Regulation: physiology,Humans,Models, Biological,Models, Statistical,Proteome,Proteome: metabolism,Signal Transduction,Signal Transduction: physiology},
month = may,
