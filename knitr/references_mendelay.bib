journal = {Bioinformatics},
keywords = {Algorithms,B-Cell,B-Cell: genetics,Computational Biology,Computational Biology: methods,Gene Expression Profiling,Gene Expression Profiling: methods,Humans,Lymphoma,Proportional Hazards Models},
month = jul,
