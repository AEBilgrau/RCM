issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
keywords = {Cell Line, Tumor,Databases, Genetic,Gene Expression Profiling,Gene Expression Profiling: methods,Genome,Genomics,Genomics: methods,Humans,Lung Neoplasms,Lung Neoplasms: genetics,Meta-Analysis as Topic,Oligonucleotide Array Sequence Analysis,Oligonucleotide Array Sequence Analysis: methods},
