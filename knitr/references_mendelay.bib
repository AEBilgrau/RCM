month = jan,
pages = {40},
pmid = {16515682},
title = {{Gene connectivity, function, and sequence conservation: predictions from modular yeast co-expression networks.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1413526\&tool=pmcentrez\&rendertype=abstract},
volume = {7},
